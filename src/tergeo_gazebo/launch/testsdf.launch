<launch>
  <arg name="x_pos" default="10.0" />
  <arg name="y_pos" default="10.0" />
  <arg name="z_pos" default="0.0" />
  <arg name="yaw" default="3.1415926" />

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find tergeo_description)/world/test_mapping.world"/>
    <arg name="paused" value="true"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_sdf"  args="-sdf -model tergeo -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw) -file $(find tergeo_description)/box/model.sdf" />
    

</launch>

